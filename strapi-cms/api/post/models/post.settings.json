{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "published",
        "draft",
        "archive"
      ]
    },
    "content": {
      "type": "richtext"
    }
  }
}
